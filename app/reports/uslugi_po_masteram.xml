    <datasources>
        <datasource id="sql" type="sql" openRole="begin" source="static" connection="internal" parentID="" connID="">SELECT 
  ROW_NUMBER() over (order by clients.fio) AS Number,
  clients.fio AS client_name,
  uslugi.name AS usluga,
  personal.fio AS master_name,
  vid_plateja.name AS vid_plateja,
  SUM(clients_history.summa) AS SUMMA,
  SUM(clients_history.summa_skidki) AS summa_skidki,
  SUM(CASE WHEN oplati_clients.type_oplati = 1 THEN
  oplati_clients.summa_opl
  ELSE
0 
  END)  AS summa_vsego
  
FROM 
  clients_history INNER JOIN personal ON personal.id = clients_history.id_sotrudnik
  INNER JOIN clients ON clients.id = clients_history.id_client
  INNER JOIN uslugi ON uslugi.id = clients_history.id_usluga
  INNER JOIN oplati_clients ON oplati_clients.Number = clients_history.NUMBER
  INNER JOIN vid_plateja ON vid_plateja.ID = oplati_clients.type_oplati
WHERE 
  clients_history.date_uslugi BETWEEN '$P{date1}' AND '$P{date2}'
  AND oplati_clients.vid_doc = 1
GROUP BY
  clients.fio,
  personal.fio, 
  vid_plateja.name,  
  clients_history.date_uslugi,
  uslugi.name
ORDER BY  
  clients.fio,
  personal.fio</datasource>
        <datasource id="sql2" type="sql" openRole="begin" source="static" connection="internal" parentID="" connID="">SELECT 
  ROW_NUMBER() over (order by personal.fio) AS Number,  
  personal.fio AS master_name,
  SUM(clients_history.summa) AS SUMMA,
  SUM(clients_history.summa_skidki) AS summa_skidki,
  SUM(CASE WHEN oplati_clients.type_oplati = 1 THEN
oplati_clients.summa_opl
  ELSE
0 
  END)  AS summa_vsego
  
FROM 
  clients_history INNER JOIN personal ON personal.id = clients_history.id_sotrudnik
  INNER JOIN oplati_clients ON oplati_clients.Number = clients_history.NUMBER
WHERE 
  clients_history.date_uslugi BETWEEN '$P{date1}' AND '$P{date2}'
  AND oplati_clients.vid_doc = 1
GROUP BY
  personal.fio,
  clients_history.summa_skidki
ORDER BY  
  personal.fio</datasource>
        <datasource id="sql3" type="sql" openRole="begin" source="static" connection="internal" parentID="" connID="">SELECT 
  ROW_NUMBER() over (order by clients.fio) AS Number,
  clients.fio AS client_name,
  materials.name AS usluga,
  personal.fio AS master_name,
  vid_plateja.name AS vid_plateja,
  SUM(clients_prodaji.count) AS count,
  SUM(clients_prodaji.summa_skidki) AS summa_skidki,
  SUM(CASE WHEN oplati_clients.type_oplati = 1 THEN
oplati_clients.summa_opl
  ELSE
0 
  END)  AS summa_vsego,
  ed_izm.name AS ed_izm
  
FROM 
  clients_prodaji INNER JOIN personal ON personal.id = clients_prodaji.id_sotrudnik
  INNER JOIN clients ON clients.id = clients_prodaji.id_client
  INNER JOIN materials ON materials.id = clients_prodaji.id_material
  INNER JOIN oplati_clients ON oplati_clients.Number = clients_prodaji.NUMBER
  INNER JOIN vid_plateja ON vid_plateja.ID = oplati_clients.type_oplati
  INNER JOIN ed_izm ON ed_izm.ID = materials.id_ed_izm
  
WHERE 
  clients_prodaji.date BETWEEN '$P{date1}' AND '$P{date2}'
  AND oplati_clients.vid_doc = 2 
GROUP BY
  clients.fio,
  personal.fio, 
  vid_plateja.name,  
  clients_prodaji.date,
  materials.name,
  ed_izm.name
ORDER BY  
  clients.fio,
  personal.fio</datasource>
        <datasource id="sql4" type="sql" openRole="begin" source="static" connection="internal" parentID="" connID="">SELECT 
  ROW_NUMBER() over (order by materials.name) AS Number,
  personal.fio AS master_name, 
  materials.name AS material, 
  SUM(sklad.count * -1) AS COUNT, 
  ed_izm.name AS ed_izm
FROM 
  sklad
  INNER JOIN materials ON materials.id = sklad.id_material
  INNER JOIN ed_izm ON materials.id_ed_izm = ed_izm.id
  INNER JOIN clients_history ON clients_history.number = sklad.number AND clients_history.date_uslugi = sklad.date
  INNER JOIN personal ON clients_history.id_sotrudnik = personal.id
  
WHERE 
  sklad.vid_doc = 1
  AND sklad.type_operacii = 2
  AND clients_history.date_uslugi BETWEEN '$P{date1}' AND '$P{date2}'
  
GROUP BY
  personal.fio, 
  materials.name, 
  ed_izm.name
</datasource>
